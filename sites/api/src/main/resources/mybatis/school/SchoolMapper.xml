<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace必须指向Dao接口 -->
<mapper namespace="com.gxb.sites.api.resource.school.dao.SchoolDao">

    <select id="getAppSchoolList" resultType="School">
        SELECT t.entry_level,s.* from school s join tenants t on s.tenant_id = t.id where s.status='online'
    </select>

</mapper>
